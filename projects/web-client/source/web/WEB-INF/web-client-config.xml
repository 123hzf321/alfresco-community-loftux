<activiti-config>
   
   <global>
      <evaluators>
         <evaluator id="string" class="com.activiti.config.evaluator.StringEvaluator" />
         <evaluator id="type-name" class="jsftest.repository.TypeNameEvaluator" />
         <evaluator id="aspect" class="com.activiti.web.config.AspectEvaluator" />
         <evaluator id="object-type" class="com.activiti.config.ObjectTypeEvaluator" />
      </evaluators>
      <element-readers>
         <!-- NOTE: This will be a built-in element reader, it's here for testing -->
         <element-reader element-name="properties" 
                         class="com.activiti.config.xml.elementreader.PropertiesElementReader" />
     </element-readers>
   </global>
   
